<UserControl 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Wpf"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    x:Class="Sand.Fhem.Home.Modules.FhemModule.Views.FhemObject.FhemObjectTitleView"
    prism:ViewModelLocator.AutoWireViewModel="True">

    <StackPanel Orientation="Horizontal">

        <Button Content="Back" Command="{Binding NavigateBackCommand}" Margin="5" />

        <TextBlock Text="{Binding FhemService.SelectedFhemObject.Name}" Style="{StaticResource FhemObjectNameTextBlockStyle}" />

    </StackPanel>
    
</UserControl>